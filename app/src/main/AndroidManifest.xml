<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ges.kumov.my.listorder">

    <application
        android:allowBackup="true"
        android:banner="@drawable/logo"
        android:icon="@drawable/logo"
        android:label="@string/app_name">


        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Info"
            android:label="@string/str_info"></activity>
        <activity
            android:name=".ClientList"
            android:label="@string/client_list"></activity>
        <activity
            android:name=".Licence"
            android:label="@string/licence"></activity>
        <activity
            android:name=".NewClient"
            android:label="@string/add_new_client"></activity>
        <activity
            android:name=".NewOrder"
            android:label="@string/new_order"></activity>
        <activity
            android:name=".ListOrder"
            android:label="@string/order_list"></activity>
    </application>

</manifest>
